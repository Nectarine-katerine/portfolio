.feedback {
   background: url(../images/content/feedback_bg.png) center center / cover no-repeat;
   position: relative;
   display: flex;
   flex-direction: column;
   &::before{
     content: "";
     position: absolute;
     top: 0;
     left: 0;
     width: 100%;
     height: 100%;
     background: linear-gradient(-90deg, transparent 0, rgba(#1b1f22, .7) 25%);
   }
}