mixin social(mod)
  div(class="social social__" + mod)
    ul.social__list
      li.social__item
        a(href="#").social__link
          +icon('insta', 'social__icon')
        li.social__item
          a(href="#").social__link
            +icon('vk', 'social__icon')
        li.social__item
          a(href="#").social__link
            +icon('github', 'social__icon')